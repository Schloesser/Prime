<BorderPane fx:id="borderpane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" scaleShape="false" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
         <right>
            <TextArea fx:id="taDisplay" editable="false">
         <BorderPane.margin>
            <Insets bottom="30.0" right="30.0" />
         </BorderPane.margin></TextArea>
         </right>
         <left>
            <VBox prefHeight="324.0" prefWidth="162.0" spacing="20.0" BorderPane.alignment="CENTER">
               <children>
                  <RadioButton fx:id="radioInteger" mnemonicParsing="false" selected="true" text="get as Integer" />
                  <RadioButton fx:id="radioString" mnemonicParsing="false" text="get as String" />
                  <RadioButton fx:id="radioObject" mnemonicParsing="false" text="get as Object" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="30.0" top="50.0" />
               </padding>
            </VBox>
         </left>
         <bottom>
            <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <items>
                  <Button fx:id="btnGo" mnemonicParsing="false" onAction="#goButtonAction" prefHeight="27.0" prefWidth="69.0" text="Go" />
                  <TextField fx:id="tfLimit" prefHeight="27.0" prefWidth="60.0" text="100" />
                  <Label prefHeight="17.0" prefWidth="86.0" text="Limit" />
               </items>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
            </ToolBar>
         </bottom>
         <top>
            <Label alignment="CENTER" text="Prime" BorderPane.alignment="CENTER">
               <font>
                  <Font size="42.0" />
               </font>
            </Label>
         </top>
      </BorderPane>
